:10px; border-top:1px solid #000; border-bottom:2px solid #000; border-left:2px solid #000; border-right:2px solid #000; display:flex; align-items:center;\"><div style=\"width:100%; text-align:center\">ENTER</div></div>\n        </div>\n      </div>\n      <div v-else-if=\"keyboardType == 'cree'\" style=\"flex:1; display:flex\">cree</div>\n      <div v-else-if=\"keyboardType == 'objiway'\" style=\"flex:1; display:flex\">objiway</div>\n      <div v-else-if=\"keyboardType == 'blackfoot'\" style=\"flex:1; display:flex\">\n        <img style=\"width:70px; height:70px; object-fit:contain\" :src=\"require('@/assets/kwa.png')\"  />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HelloWorld',\n  data() {\n    return {\n      from:'eng',\n      to:'eng',\n      keyboard:false,\n      engRow1:['Q','W','E','R','T','Y','U','I','O','P'],\n      engRow2:['A','S','D','F','G','H','J','K','L'],\n      engRow3:['Z','X','C','V','B','N','M'],\n      userInput:'',\n      keyboardType:'eng',\n      visible:false,\n      newWord:{\n        eng:'',\n        translationType:'',\n        translation:'',\n        id:''\n      }\n    }\n  },\n  methods:{\n    swap() {\n      let temp = this.from\n      this.to = temp\n      this.from = this.to\n    },\n    input(letter) {\n      if(letter != 'del') {\n        this.userInput = this.userInput + letter\n      } else if (letter == 'del') {\n        this.userInput = this.userInput.slice(0, -1)\n      }\n    },\n    addNew() {\n      this.visible = true\n    },\n    onClose() {\n      this.visible = false\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.keyboard:hover{\n  background-color:#000;\n  cursor:pointer;\n  color:#FFF;\n}\n.keyboard{\n  font-weight:600;\n  font-size:20px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}